// v.end() means element after the last element

#include <iostream>
#include <vector>
using namespace std;

int main()
{
    vector<int> v{10,20,30,40};
    v.erase(v.begin());
    for(auto x:v)
    {
        cout<<x<<" ";
    }
    cout<<endl;
    v.erase(v.begin(),v.end()-2);
    for(auto s:v)
    {
        cout<<s<<" ";
    }
    return 0;
}
