
#include <iostream>
#include <vector>
using namespace std;

int main()
{
    vector<int> v{10,20,30,40};
     v.insert(v.begin(),100);
     v.insert(v.begin()+2,2000);
     for(auto x:v)
     {
         cout<<x<<" ";
     }
     cout<<"\n";
     vector<int> v2;
     v2.insert(v2.begin(),v.begin(),v.begin()+2);
     for(auto c:v2)
     {
         cout<<c<<" ";
     }
    return 0;
}
