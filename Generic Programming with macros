
#include <iostream>

#define MAX_SIZE 100

#define PI 3.14

using namespace std;

int main()
{
    int num;
    cout<<"Enter a number"<<endl;
    cin>>num;
    if(num>100)
    {
        cout<<"Number is too Big"<<endl;
    }
    else
    {
        cout<<"Number is small"<<endl;
    }
    double area=PI*2*2;
    cout<<"Area "<<area<<endl;

    return 0;
}
